<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 하단바의 홈이 눌렸을 때 selected (진한 icon) -->
    <item android:state_checked="true" android:drawable="@drawable/icn_home_selected"/>
    <!-- 하단바의 홈이 눌리지 않았을 때 normal (연한 icon) -->
    <item android:state_checked="false" android:drawable="@drawable/icn_home_normal"/>
</selector>