<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/menu_home">

    <fragment
        android:id="@+id/menu_home"
        android:name="com.sopt.now.jumpit.feature.home.HomeFragment"
        android:label="홈"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/actionHomeToResume"
            app:destination="@id/menu_resume" />
    </fragment>

    <fragment
        android:id="@+id/menu_resume"
        android:name="com.sopt.now.jumpit.feature.resume.ResumeFragment"
        android:label="이력서"
        tools:layout="@layout/fragment_resume">
        <action
            android:id="@+id/actionResumetoHome"
            app:destination="@id/menu_home"/>
